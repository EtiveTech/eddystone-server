import org.etive.city4age.repository.Device
import org.etive.city4age.repository.Track

model {
	Track track
}

json {
	id track.id
	latitude track.latitude
	longitude track.longitude
	accuracy track.accuracy
	timestamp track.timestamp
	timeAtLocation Math.round(((track.timeAtLocation / 60) * 100) as Float) / 100
	triggeredBy track.triggeredBy
	careReceiver track.careReceiver.id
	device Device.getId(track.device)
	dateCreated track.dateCreated
}